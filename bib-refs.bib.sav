@PHDTHESIS{nehrkorn,
      author       = {Nehrkorn, Alexander Rafael},
      othercontributors = {Stahl, Achim and Lohmann, Wolfgang},
      title        = {{S}earch for the standard model {H}iggs boson decaying into
                      pairs of tau leptons with the {CMS} {E}xperiment},
      school       = {RWTH Aachen University},
      type         = {Dissertation},
      address      = {Aachen},
      reportid     = {RWTH-2017-09574},
      pages        = {1 Online-Ressource (xi, 132 Seiten) : Illustrationen,
                      Diagramme},
      year         = {2017},
      note         = {Veröffentlicht auf dem Publikationsserver der RWTH Aachen
                      University; Dissertation, RWTH Aachen University, 2017},
      abstract     = {The search for the standard model Higgs boson decaying into
                      pairs of tau leptons is described. A data set of
                      proton-proton collisions at a center-of-mass energy of 13
                      TeV corresponding to an integrated luminosity of
                      $35.9$fb$^{-1}$ collected with the CMS detector at the CERN
                      LHC is used. Particular emphasis is placed on developments
                      concerning the reconstruction and identification of tau
                      leptons. An analysis of the reconstructed tau lepton energy
                      in data and simulation is performed, and corrections to the
                      simulation are determined. Prospects to increase the
                      precision of this measurement are discussed. The published
                      results of the search for the decay $\mathrm{H}\to\tau\tau$
                      are compared to results obtained in this thesis with and
                      without using an updated version of the tau lepton
                      identification. Additionally, a search for decays of the Z
                      boson into an electron and a muon is described.},
