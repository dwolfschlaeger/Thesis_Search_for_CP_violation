@article{1742-6596-513-2-022035,
  author={Lorenzo Bianchini and John Conway and Evan Klose Friis and Christian Veelken},
  title={Reconstruction of the Higgs mass in H â ÏÏ Events by Dynamical Likelihood techniques},
  journal={Journal of Physics: Conference Series},
  volume={513},
  number={2},
  pages={022035},
  url={http://stacks.iop.org/1742-6596/513/i=2/a=022035},
  year={2014},
  abstract={An algorithm for reconstruction of the Higgs mass in H â ÏÏ decays is presented. The algorithm computes for each event a likelihood function P ( M ÏÏ ) which quantifies the level of compatibility of a Higgs mass hypothesis M ÏÏ with measured momenta of the visible tau decay products plus the missing transverse energy reconstructed in the event. The algorithm is used in the CMS H â ÏÏ analysis, where it is found to improve the sensitivity to discover the Standard Model Higgs boson in this decay channel by about 30%.}
}
