      ddc          = {530},
      cid          = {$I:(DE-82)130000_20140620$ / $I:(DE-82)133510_20140620$},
      typ          = {PUB:(DE-HGF)11},
      doi          = {10.18154/RWTH-2017-09574},
      url          = {https://publications.rwth-aachen.de/record/707995},
}

